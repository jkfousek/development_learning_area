{
  "editor.fontFamily": "Fira Code",
  "editor.fontWeight": "900",
  "editor.fontSize": 15,
  "editor.colorDecorators": true,
  "editor.fontLigatures": true,
  "editor.inlayHints.enabled": "off",
  "workbench.colorCustomizations": {
    "editor.selectionHighlightBorder": "#333",
    "editor.selectionBackground": "#333",
    "editor.lineHighlightBackground": "#22262c",
    "editor.lineHighlightBorder": "#469",
    "editor.background": "#333333",
    "editorGroup.border": "#469f",
    "editor.hoverHighlightBackground": "#33333399",
    "editor.foreground": "#469"
  },
  "editor.wordWrap": "on",
  "diffEditor.wordWrap": "off",
  "editor.guides.indentation": false,
  "editor.guides.bracketPairs": false,
  "editor.tabSize": 3,
  "files.autoSave": "afterDelay",
  "editor.autoIndent": "advanced",
  "editor.minimap.enabled": false,
  "editor.tokenColorCustomizations": {
    "keywords": "#60acee99",
    "strings": "#60acee99",
    "comments": "#33993399",
    "types": "#60acee99",
    "numbers": "#5bc1f0",
    "functions": "#b857b3fb",
    "variables": "#e0e3e7",
    "textMateRules": [
      {
        "scope": "keyword.operator",
        "settings": {
          "foreground": "#859ca7",
          "fontStyle": "bold"
        }
      }
    ]
  },
  "conventionalCommits.lineBreak": "\\n",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.linkedEditing": true,
  "git.openRepositoryInParentFolders": "never",
  "files.associations": {
    "*.embeddedhtml": "html",
    "*.rmd": "markdown"
  },
  "workbench.sideBar.location": "right",
  "text"
}
